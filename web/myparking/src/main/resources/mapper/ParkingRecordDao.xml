<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.intelligentparking.myparking.DAO.ParkingRecordDao">

    <insert id="addParkingRecord">
        insert into parkingrecord (carLicence,startTime,endTime,fee,uid)
		    values(#{carLicence},#{startTime},#{endTime},#{fee},#{uid})
    </insert>

</mapper>